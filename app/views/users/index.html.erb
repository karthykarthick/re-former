<h1>All users</h1>
<%= link_to "Create a New User", new_user_path %>
<ul id="articles">
    <% @users.each do |user| %>
    <li>
        <%= 'Username = ' + user.username  %><br>
        <%= 'Email = ' + user.email  %><br>  <br>   
        <%= link_to "Edit User", edit_user_path(user.id) %>
     
    </li>

    <% end %>
</ul>
